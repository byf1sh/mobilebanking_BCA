<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mTransferActivity"
    android:background="@drawable/mtransfermbca">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/daftartransfer"
        android:layout_width="986px"
        android:layout_height="211px"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="523px">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/dtransfer_AntarRekening"
            android:layout_width="match_parent"
            android:layout_height="100px"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toLeftOf="parent">
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.cardview.widget.CardView
            android:id="@+id/dtransfer_AntarBank"
            android:layout_width="match_parent"
            android:layout_height="100px"
            app:layout_constraintTop_toBottomOf="@id/dtransfer_AntarRekening"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:visibility="invisible">
        </androidx.cardview.widget.CardView>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/transfer"
        android:layout_width="986px"
        android:layout_height="600px"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/daftartransfer"
        android:layout_marginBottom="856px"
        android:layout_marginStart="-8px"
        android:visibility="invisible">
        <androidx.cardview.widget.CardView
            android:id="@+id/transfer_AntarRekening"
            android:layout_width="match_parent"
            android:layout_height="100px"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toTopOf="@id/transfer_AntarBank"
            android:visibility="invisible">
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/transfer_AntarBank"
            android:layout_width="match_parent"
            android:layout_height="100px"
            app:layout_constraintTop_toBottomOf="@id/transfer_AntarRekening"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toTopOf="@id/transfer_BCAVA"
            android:visibility="invisible">
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/transfer_BCAVA"
            android:layout_width="match_parent"
            android:layout_height="100px"
            app:layout_constraintTop_toBottomOf="@id/transfer_AntarBank"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toTopOf="@id/transfer_Sakuku"
            app:layout_constraintLeft_toLeftOf="parent"
            android:visibility="invisible">
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/transfer_Sakuku"
            android:layout_width="match_parent"
            android:layout_height="100px"
            app:layout_constraintTop_toBottomOf="@id/transfer_BCAVA"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toTopOf="@id/transfer_StatusTransaksiSakuku"
            app:layout_constraintLeft_toLeftOf="parent"
            android:visibility="invisible">
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/transfer_StatusTransaksiSakuku"
            android:layout_width="match_parent"
            android:layout_height="100px"
            app:layout_constraintTop_toBottomOf="@id/transfer_Sakuku"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toTopOf="@id/transfer_inbox"
            app:layout_constraintLeft_toLeftOf="parent"
            android:visibility="invisible">

        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/transfer_inbox"
            android:layout_width="match_parent"
            android:layout_height="100px"
            app:layout_constraintTop_toBottomOf="@id/transfer_StatusTransaksiSakuku"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:visibility="invisible">
        </androidx.cardview.widget.CardView>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>